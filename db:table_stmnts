CREATE DATABASE FunFriends33;


CREATE TABLE carsForSale (
	VIN varchar(30) primary key,
	Price int,
	Year year,
	Make varchar(30),
	Model varchar(30),
	Body_Style varchar(30),
	Mileage int,
	Transmission varchar(30),
	Engine varchar(30),
	Drivetrain varchar(30),
	Exterior varchar(30),
	Interior varchar(30),
	Doors int,
	Stock varchar(30),
	Fuel_Mileage varchar(30),
	Conditon varchar(30));